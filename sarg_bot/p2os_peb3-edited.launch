<launch>
  <master auto="start"/>

  <!-- use sim time for /clock -->
  <param name="use_sim_time" value="true"/>

	<!-- run p2os to control the pioneer -->
  <arg name="p2os_driver" value="1"/>

  <!-- start stage in the given world -->
  <param name="includeFile" value="$(find sarg_2dnav)/peb3-edited.world" type="str"/>
  <node pkg="stage_ros" type="stageros" name="stageros" args="$(find sarg_2dnav)/peb3-edited.world"/>
</launch>
